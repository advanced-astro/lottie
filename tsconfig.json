{
	"compilerOptions": {
		"outDir": "dist",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "Node",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"module": "ESNext",
		"target": "ESNext",
		"declaration": true
	},
	"include": [
        "src"
	]
}
